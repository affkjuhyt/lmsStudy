lesson = $(".lessons-list .card-lesson-show").get(<%= @lesson.sequence %> - 1);
$(".lessons-list").find(".card-body").removeClass("active");
$(lesson).find(".card-body").addClass("active");
$(".lesson-content").html("<%= j render 'lessons/video'%>");
<% if @lesson.video? %>
  $(".lesson-content").html("<%= j render 'lessons/video'%>");
<% elsif @lesson.quiz? %>
  $(".lesson-content").html("<%= j render 'lessons/question'%>");
<% end %>

